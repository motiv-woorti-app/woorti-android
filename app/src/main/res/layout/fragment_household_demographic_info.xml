<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout29"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FBE5CD"
        tools:context=".settingsAndProfile.fragments.demographicInfo.HouseholdDemographicInfoFragment">

        <!-- TODO: Update blank fragment layout -->

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="24dp"
            android:background="@drawable/profile_and_settings_general_shape"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <EditText
                android:id="@+id/peopleEditText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/mytrips_objective_of_trip_other_text_field_shape"
                android:clickable="true"
                android:ellipsize="end"
                android:ems="10"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:hint="@string/Number_People_Household"
                android:inputType="textNoSuggestions"
                android:maxLines="2"
                android:paddingStart="12dp"
                android:paddingTop="12dp"
                android:paddingEnd="12dp"
                android:paddingBottom="12dp"
                android:selectAllOnFocus="false"
                android:singleLine="false"
                android:textColor="#ED7E03"
                android:textColorHint="#70706D"
                app:layout_constraintBottom_toBottomOf="@+id/textView72"
                app:layout_constraintEnd_toEndOf="@+id/maritalEditText"
                app:layout_constraintStart_toStartOf="@+id/maritalEditText"
                app:layout_constraintTop_toTopOf="@+id/textView72" />

            <EditText
                android:id="@+id/yearsResidenceEditText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/mytrips_objective_of_trip_other_text_field_shape"
                android:clickable="true"
                android:ellipsize="end"
                android:ems="10"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:hint="@string/Household_Years_Residence"
                android:inputType="textNoSuggestions"
                android:maxLines="2"
                android:paddingStart="12dp"
                android:paddingTop="12dp"
                android:paddingEnd="12dp"
                android:paddingBottom="12dp"
                android:selectAllOnFocus="false"
                android:textColor="#ED7E03"
                android:textColorHint="#70706D"
                app:layout_constraintBottom_toBottomOf="@+id/textView84"
                app:layout_constraintEnd_toEndOf="@+id/maritalEditText"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/maritalEditText"
                app:layout_constraintTop_toTopOf="@+id/textView84" />

            <EditText
                android:id="@+id/labourEditText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@drawable/mytrips_objective_of_trip_other_text_field_shape"
                android:clickable="true"
                android:ellipsize="end"
                android:ems="10"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:hint="@string/Household_Labour_Status"
                android:inputType="textNoSuggestions"
                android:maxLines="2"
                android:paddingStart="12dp"
                android:paddingTop="12dp"
                android:paddingEnd="12dp"
                android:paddingBottom="12dp"
                android:selectAllOnFocus="false"
                android:singleLine="false"
                android:textColor="#ED7E03"
                android:textColorHint="#70706D"
                app:layout_constraintBottom_toBottomOf="@+id/textView83"
                app:layout_constraintEnd_toEndOf="@+id/maritalEditText"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/maritalEditText"
                app:layout_constraintTop_toTopOf="@+id/textView83" />

            <EditText
                android:id="@+id/maritalEditText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="24dp"
                android:background="@drawable/mytrips_objective_of_trip_other_text_field_shape"
                android:clickable="true"
                android:ellipsize="end"
                android:ems="10"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:hint="@string/Household_Marital"
                android:inputType="textNoSuggestions"
                android:maxLines="2"
                android:paddingStart="12dp"
                android:paddingTop="@dimen/value12dp"
                android:paddingEnd="12dp"
                android:paddingBottom="@dimen/value12dp"
                android:selectAllOnFocus="false"
                android:singleLine="false"
                android:textColor="#ED7E03"
                android:textColorHint="#70706D"
                app:layout_constraintBottom_toBottomOf="@+id/textView25"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/textView25"
                app:layout_constraintTop_toTopOf="@+id/textView25" />

            <Button
                android:id="@+id/doneFinalButton"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="@dimen/value24dp"
                android:layout_marginEnd="24dp"
                android:layout_marginBottom="24dp"
                android:background="@drawable/profile_and_settings_edit_button_shape"
                android:fontFamily="@font/montserrat_bold"
                android:text="@string/End"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/labourEditText"
                app:layout_constraintVertical_bias="1.0" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="@dimen/value40dp"
                android:gravity="center|start"
                android:minLines="2"
                android:text="@string/Household_Marital"
                android:textSize="18sp"
                app:layout_constraintEnd_toStartOf="@id/maritalEditText"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView72"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="8dp"
                android:gravity="center|start"
                android:minLines="3"
                android:text="@string/Number_People_Household"
                android:textSize="18sp"
                app:layout_constraintEnd_toStartOf="@+id/peopleEditText"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textView25"
                app:layout_constraintTop_toBottomOf="@+id/maritalEditText" />

            <TextView
                android:id="@+id/textView83"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="8dp"
                android:gravity="center|start"
                android:minLines="3"
                android:text="@string/Household_Labour_Status"
                android:textSize="18sp"
                app:layout_constraintEnd_toStartOf="@+id/labourEditText"
                app:layout_constraintStart_toStartOf="@+id/textView25"
                app:layout_constraintTop_toBottomOf="@+id/yearsResidenceEditText" />

            <TextView
                android:id="@+id/textView84"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="8dp"
                android:gravity="center|start"
                android:minLines="3"
                android:text="@string/Household_Years_Residence"
                android:textSize="18sp"
                app:layout_constraintEnd_toStartOf="@+id/yearsResidenceEditText"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textView25"
                app:layout_constraintTop_toBottomOf="@+id/peopleEditText" />

        </android.support.constraint.ConstraintLayout>
    </android.support.constraint.ConstraintLayout>
</ScrollView>